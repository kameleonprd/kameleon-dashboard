NEXT_PUBLIC_COGNITO_CLIENT_ID=XXXXXXXXXX
NEXT_PUBLIC_AWS_REGION=us-east-1

# Site URL (for OAuth callbacks if needed)
NEXT_PUBLIC_SITE_URL=http://localhost:3000
